{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": ["ES2019", "DOM"],
    "outDir": "dist",
    "noEmitOnError": false
  },
  "include": [
    "Patterns/**/*.ts"
  ]
}


